<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/scenario.xsd -->
<scenario>
    <function name="handleIdentity">
        <input>
            <parameter name="[function:nas_pdu_msg]" />
        </input>
    
        <do>    
            <call name="identityReqsponse" >
                <input>
                    <parameter name="[function:mobile_id]"  value="1111"/>
                </input>

                <output>
                    <parameter name="[function:id_msgd]" /> 
                </output>
            </call>

            <parameter name="[send_nas_pdu_msgd]" operation="set" value="[id_msgd]"/>
            <call name="uplinkNas" >
                <input>
                    <parameter name="[function:nas_pdu_msg]"  value="[send_nas_pdu_msgd]"/>
                </input>
            </call> 

            <parameter name="[out_ret]" operation="set" value="true"/>   
        </do>
      
        <output>
            <parameter name="[function:out_ret]" />
        </output>
    </function>
</scenario>
