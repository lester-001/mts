<?xml version="1.0" encoding="UTF-8" standalone="no"?>


<test description="NGAP regression tests" name="NGAP">

    <!-- NOTE : If you will run the test suite on LUNIX platform then you have to change 
         the following parameter in the ngap.properties file :
         DEFAULT_SERVER_URL = aaa://<your IP address> :3871 
    -->

    <parameter name="[IPVersion]" operation="set" value="ipv4" description="version of the IP protocol" editable="true" />
    
    <!--parameter name="[localHostNGAP]" operation="system.ipaddress" value2="[IPVersion]"/-->
    <parameter name="[localHostNGAP]" operation="set" value="192.168.21.211" description="local address of the MTS host" editable="true"/>
    <!--parameter name="[localPortNGAP]" operation="file.readproperty" value="ngap.properties" value2="listenpoint.LOCAL_PORT"/-->
    <parameter name="[localPortNGAP]" operation="set" value="38412"/>
    <parameter name="[localPortNGAP]" operation="set" value="[localPortNGAP]" description="local port of the MTS host" editable="true"/>
    <parameter name="[localNGAP]" operation="set" value="[localHostNGAP]:[localPortNGAP]"/>
    <parameter name="[localRealmNGAP]" operation="set" value="client.fr"/>
    <parameter name="[localClient]" operation="set" value="true" description="flag to activate the client scenario" editable="true" />
    
    <!--parameter name="[remoteHostNGAP]" operation="system.ipaddress" value2="[IPVersion]"/-->
    <!--parameter name="[remoteHostNGAP]" operation="set" value="FR00115331"/-->
    <parameter name="[remoteHostNGAP]" operation="set" value="192.168.21.210"/>
    <!--parameter name="[remoteHostNGAP]" operation="set" value="2a01:cf00:33:5::6e01"/-->
    <parameter name="[remoteHostNGAP]" operation="set" value="[remoteHostNGAP(0)]" description="remote address of the SUT host" editable="true"/>
    <parameter name="[remotePortNGAP]" operation="file.readproperty" value="ngap.properties" value2="listenpoint.LOCAL_PORT"/>
    <!--parameter name="[remotePortNGAP]" operation="set" value="3868"/-->
    <parameter name="[remotePortNGAP]" operation="set" value="[remotePortNGAP]" description="remote port of the SUT host" editable="true"/>
    <parameter name="[remoteNGAP]" operation="set" value="[remoteHostNGAP]:[remotePortNGAP]"/>
    <!--parameter name="[remoteRealmNGAP]" operation="set" value="client.fr"/>
    <parameter name="[remoteServer]" operation="set" value="true" description="flag to activate the server scenario" editable="true"/-->
    
    <parameter name="[iterations]" operation="set" value="1" description="iterations number" editable="true"/>
    <parameter name="[timeout]" operation="set" value="300" description="timeout duration for message receipt : 0 mean always" editable="true"/>
    
    <parameter name="[transport]" operation="file.readproperty" value="ngap.properties" value2="listenpoint.TRANSPORT"/>

    <parameter name="[mcc]" value="1" />

    <parameter name="[SST]" value="01" />
    <parameter name="[SD]" value="000001" />

    <parameter name="[RAN_UE_NGAP_ID]" value="37" />
    <parameter name="[AMF_UE_NGAP_ID]" value="8" />
    <parameter name="[NAS_PDU_CONTENT]" value="" />   

    <parameter name="[ngap_ch_name]" value="" />
        
    <parameter name="[gnb_plmn_id]" value="00F110" />
    <parameter name="[gnb_tac]" value="000001" />
    <parameter name="[gnb_cell_id]" value="000000000000000000000000000000000001" />
    <parameter name="[gnb_id]" value="00000000000000000000000000000000" />

    <parameter name="[ue_op_type]" value="OPC" />
    <parameter name="[ue_op]"  value="279EB54971771559879284FDDDE3EE0C" />
    <parameter name="[ue_key]" value="00112233445566778899AABBCCDDEEFF" />
    <parameter name="[ue_key_amf]" value="" />
    <parameter name="[ue_key_ausf]" value="" />
    <parameter name="[ue_key_seaf]" value="" />
    <parameter name="[ue_supi]" value="001010000000001" />
    <parameter name="[ue_snn]" value="5G:mnc001.mcc001.3gppnetwork.org" />
    <parameter name="[ue_selected_int_alg]" value="" />
    <parameter name="[ue_selected_enc_alg]" value="" />
    <parameter name="[ue_selected_kNasEnc]" value="" />
    <parameter name="[ue_selected_kNasInt]" value="" />
    <parameter name="[ue_qos_flow_id]" value="1" />
    <parameter name="[ue_pdu_session_id]" value="1" />
    <parameter name="[ue_transportLayer_address]" value="11000000101010000001010100011110" />
    <parameter name="[ue_gtp_teid]" value="1400000f" />

    <parameter name="[isUplink]" value="0" />

    <parameter name="[uplinkCount_sqn]" value="0" />
    <parameter name="[uplinkCount_overflow]" value="0" />
    
    <parameter name="[test_result]" value="true"/>  

    <testcase name="NgapTest" state="true" number="[iterations]" description="NGAPeter" >
        <!--scenario name="client" file="testcase/ngsetup.xml" routingName="[localPortNGAP]"/-->
        <scenario name="client" file="testcase/RegistrationRequest.xml" routingName="[localPortNGAP]"/>
    </testcase>
</test>
